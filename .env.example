# EquiXtate Environment Variables

# ===================================
# Privy Authentication
# ===================================
# Get your Privy App ID from: https://dashboard.privy.io/
VITE_PRIVY_APP_ID=your_privy_app_id_here

# ===================================
# WalletConnect
# ===================================
# Get your project ID from: https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# ===================================
# Groq AI API (for AI Advisor)
# ===================================
# Get your API key from: https://console.groq.com/
VITE_GROQ_API_KEY=your_groq_api_key_here

# ===================================
# KRNL Protocol Configuration
# ===================================
# KRNL RPC endpoint for verification services
VITE_RPC_KRNL=https://v0-1-0.node.lat/
NEXT_PUBLIC_RPC_KRNL=https://rpc.krnl.io

# KRNL Entry ID (from KRNL Dashboard)
VITE_KRNL_ENTRY_ID=your_entry_id_here
NEXT_PUBLIC_ENTRY_ID=your_entry_id_here

# KRNL Access Token (from KRNL Dashboard)
VITE_KRNL_ACCESS_TOKEN=your_access_token_here
NEXT_PUBLIC_ACCESS_TOKEN=your_access_token_here

# KRNL Kernel ID for property verification (default: 1529)
VITE_KRNL_KERNEL_ID=1529
NEXT_PUBLIC_KERNEL_ID=1529

# Smart Contract Address for KRNL interactions
VITE_CONTRACT_ADDRESS=your_contract_address_here
NEXT_PUBLIC_CONTRACT_ADDRESS=your_contract_address_here

# ===================================
# Additional Configuration
# ===================================
# Add any additional environment variables below

# ===================================
# Sumsub (KYC/AML)
# ===================================
# API base URL (use default unless instructed by Sumsub)
VITE_SUMSUB_API_URL=https://api.sumsub.com
# App Token from Sumsub dashboard (server-side usage via proxy)
VITE_SUMSUB_APP_TOKEN=your_sumsub_app_token_here
# Secret Key from Sumsub dashboard (server-side only; never exposed client-side)
VITE_SUMSUB_SECRET_KEY=your_sumsub_secret_key_here
# Sandbox mode flag (true for testing, false for production)
VITE_SUMSUB_SANDBOX_MODE=true

# ===================================
# KYC Verifier Contract (Blockchain)
# ===================================
# KYC Verifier contract address (deployed on Sepolia/mainnet)
VITE_KYC_VERIFIER_CONTRACT=your_kyc_verifier_contract_address
KYC_VERIFIER_CONTRACT=your_kyc_verifier_contract_address

# ===================================
# Property Registry Contract
# ===================================
# PropertyRegistry contract address for property listings
VITE_PROPERTY_REGISTRY_ADDRESS=your_property_registry_contract_address
PROPERTY_REGISTRY_ADDRESS=your_property_registry_contract_address

# ===================================
# Oracle Configuration
# ===================================
# Oracle wallet private key (backend only - writes KYC status on-chain)
# This should be a separate wallet with only oracle permissions
ORACLE_PRIVATE_KEY=0x_your_oracle_private_key_here

# Oracle wallet address (for contract configuration)
ORACLE_ADDRESS=your_oracle_wallet_address

# RPC URL for oracle transactions (Sepolia testnet)
SEPOLIA_RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# ===================================
# Optional Chain/Service Settings
# ===================================
# Etherscan API (optional, for verifications)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
# Pimlico (optional bundler)
VITE_PIMLICO_API_KEY=your_pimlico_api_key_here
# General RPC URL (optional)
VITE_RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

